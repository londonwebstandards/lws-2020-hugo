@import url(normalize.css);@font-face{font-family:"Oswald";src:url("/fonts/Oswald/static/Oswald-Regular.ttf");font-weight:400}@font-face{font-family:"Oswald";src:url("/fonts/Oswald/static/Oswald-Bold.ttf");font-weight:700}@font-face{font-family:"Playfair Display";src:url("/fonts/Playfair_Display/static/PlayfairDisplay-Regular.ttf");font-weight:400}@font-face{font-family:"Playfair Display";src:url("/fonts/Playfair_Display/static/PlayfairDisplay-Italic.ttf");font-style:italic;font-weight:400}@font-face{font-family:"Playfair Display";src:url("/fonts/Playfair_Display/static/PlayfairDisplay-Bold.ttf");font-weight:700}@font-face{font-family:"Playfair Display";src:url("/fonts/Playfair_Display/static/PlayfairDisplay-BoldItalic.ttf");font-style:italic;font-weight:700}*{box-sizing:border-box}body{background:rgba(190,215,60,0.1);border-top:5px solid #0099bd;box-sizing:border-box;font-family:"Playfair Display"}a{color:#ee2a7b}a:active,a:focus{outline:1px dotted #ee2a7b}a:hover{color:#0c372d;text-decoration:none}a:visited{color:#2b3990}.highlight{background:#fee110;padding:.25rem .5rem}.narrow{margin:0 auto;max-width:960px}button,.button{border-bottom:5px solid #e1e1e1;display:inline-block;font-family:"Oswald";padding:.5rem 1rem;text-decoration:none;text-transform:uppercase}button.primary,.button.primary{background:#ee2a7b;color:white}button.primary:hover,.button.primary:hover{border-bottom:5px solid #bed73c}button.secondary,.button.secondary{background:rgba(190,215,60,0.4)}button.secondary:hover,.button.secondary:hover{border-bottom:5px solid #ee2a7b}button.expanded,.button.expanded{display:block}.icon svg{fill:#ee2a7b;max-width:1.25rem;vertical-align:text-bottom}.icon svg:hover{fill:#2b3990}.pretty-list{list-style:none;padding:0}.pretty-list li:not(:last-child){border-bottom:1px solid #e1e1e1}header,main,footer{padding:1rem}header,footer{text-align:center}#content{margin:0 auto;max-width:1280px}h1,h2,h3,h4,h5,h6,nav{font-family:"Oswald"}.page-title{border-bottom:5px solid #e1e1e1}header{background:#f8f8f8}header a{color:black;text-decoration:none}header a:hover{text-decoration:underline}header a:visited{color:black}header .logo{display:block}.site-title{font-weight:400;text-transform:uppercase}nav{background:#e1e1e1;text-align:center}nav a{display:block;padding:1rem;text-decoration:none}nav a:hover{background:#0099bd;color:#fff}nav .menu{list-style:none;margin:0;padding:0}nav .menu li{display:inline-block;margin-left:-4px}nav .menu li:not(:last-child){border-right:3px solid #f8f8f8}nav .menu .active{border-bottom:5px solid}main{background:#f8f8f8;color:#474747;line-height:1.75;padding-bottom:2rem}.by-line{font-size:.875rem;font-style:italic;margin:0 0 .5rem}.post-title{margin:0}.event{margin-bottom:2rem;outline:1px solid #e1e1e1;padding:.5rem 2rem}.event.latest{border-left:8px solid #ee2a7b;font-size:1.25rem}.event.past{border-left:8px solid rgba(238,42,123,0.3);background:rgba(225,225,225,0.5)}.alert{background:rgba(238,42,123,0.3);border:1px solid #ee2a7b;border-radius:4px;font-size:1.25rem}footer{border-top:1px solid rgba(31,138,113,0.2)}@media screen and (min-width: 641px){.half{display:grid;grid-template-columns:repeat(2, 1fr);grid-gap:28px;grid-auto-rows:minmax(100px, auto);grid-row-gap:0px}}

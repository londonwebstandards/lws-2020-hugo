@import url(normalize.css);@font-face{font-family:"Oswald";src:url("/fonts/Oswald/static/Oswald-Regular.ttf");font-weight:400}@font-face{font-family:"Oswald";src:url("/fonts/Oswald/static/Oswald-Bold.ttf");font-weight:700}@font-face{font-family:"Playfair Display";src:url("/fonts/Playfair_Display/static/PlayfairDisplay-Regular.ttf");font-weight:400}@font-face{font-family:"Playfair Display";src:url("/fonts/Playfair_Display/static/PlayfairDisplay-Italic.ttf");font-style:italic;font-weight:400}@font-face{font-family:"Playfair Display";src:url("/fonts/Playfair_Display/static/PlayfairDisplay-Bold.ttf");font-weight:700}@font-face{font-family:"Playfair Display";src:url("/fonts/Playfair_Display/static/PlayfairDisplay-BoldItalic.ttf");font-style:italic;font-weight:700}*{box-sizing:border-box}body{background:rgba(31,138,113,0.1);border-top:5px solid #0099bd;box-sizing:border-box;font-family:"Playfair Display"}a{color:#ee2a7b}a:active,a:focus{outline:1px dotted #ee2a7b}a:hover{color:#0c372d}a:visited{color:#2b3990}.highlight{background:#fee110;padding:.25rem .5rem}.narrow{margin:0 auto;max-width:960px}button,.button{font-family:"Oswald";padding:.5rem 1rem;text-decoration:none;text-transform:uppercase}button.primary,.button.primary{background:#ee2a7b;color:white}button.primary:hover,.button.primary:hover{border-bottom:5px solid #bed73c}button.secondary,.button.secondary{background:#bed73c}button.secondary:hover,.button.secondary:hover{border-bottom:5px solid #ee2a7b}.icon svg{fill:#ee2a7b;max-width:1.25rem;vertical-align:text-bottom}.icon svg:hover{fill:#2b3990}header,main,footer{padding:1rem}#content{margin:0 auto;max-width:1280px}header,footer{text-align:center}h1,h2,h3,h4,h5,h6,nav{font-family:"Oswald"}header{background:#f8f8f8}header a{color:black;text-decoration:none}header a:hover{text-decoration:underline}header a:visited{color:black}.site-title{font-weight:400;text-transform:uppercase}nav{background:#e1e1e1;text-align:center}nav a{display:block;padding:1rem;text-decoration:none}nav a:hover{background:#0099bd;color:#fff}nav .menu{list-style:none;margin:0;padding:0}nav .menu li{display:inline-block;margin-left:-4px}nav .menu li:not(:last-child){border-right:3px solid #f8f8f8}nav .menu .active{border-bottom:5px solid}main{background:#f8f8f8;color:#474747;line-height:1.5;padding-bottom:2rem}.event{border-left:15px solid #ee2a7b;outline:1px solid #e1e1e1;padding:.5rem 2rem}footer{border-top:1px solid rgba(31,138,113,0.2)}
